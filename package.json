{
  "name": "form-forge-rag-server",
  "version": "0.0.1",
  "description": "RAG API server using Express.js, TypeScript, LangChain.js and Supabase",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc && cp -r src/prompts dist/",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "generate-async-spec": "pnpm dlx ts-node scripts/generate-asyncapi.ts",
    "generate-websocket-docs": "pnpm dlx @asyncapi/cli generate fromTemplate asyncapi.json @asyncapi/html-template@3.0.0 -o docs/websocket --use-new-generator",
    "fix-websocket-docs": "node scripts/fix-asyncapi-csp.js",
    "generate-websocket-docs-with-csp": "pnpm generate-websocket-docs && pnpm fix-websocket-docs"
  },
  "keywords": [
    "rag",
    "express",
    "typescript",
    "langchain",
    "supabase",
    "openapi"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/community": "^0.3.42",
    "@langchain/core": "^0.3.53",
    "@langchain/openai": "^0.5.10",
    "@supabase/supabase-js": "^2.49.4",
    "@types/formidable": "^3.4.5",
    "@types/pdf-parse": "^1.1.5",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "file-type": "^20.5.0",
    "formidable": "^3.5.4",
    "helmet": "^8.1.0",
    "langchain": "^0.3.24",
    "markdown-it": "^14.1.0",
    "morgan": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/markdown-it": "^14.1.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "glob": "^11.0.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
